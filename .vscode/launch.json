{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug App for C++ debugging",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/out/Debug/x64/WebViewExample/WebViewExample.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "path",
                    // customize for your buid location
                    "value": "%path%;e://ana//src//out//debug_x64"
                }
            ],
            "externalConsole": false
        },
        {
            "name": "Scenario 1: Debug App for Script debugging (first) (old adapter)",
            "type": "edge",
            "request": "launch",
            "webRoot": "${workspaceFolder}/webviewexample/src",
            "pathMapping": {
                "E:/github/WebViewExample": "${workspaceFolder}"
            },
            //"runtimeExecutable": "${workspaceFolder}/out/Debug/x64/WebViewExample/WebViewExample.exe",
            "runtimeExecutable": "E:/github/WebViewExample/out/Debug/x64/WebViewExample/WebViewExample.exe",
            "env": {
                    // customize for your build location
                    "Path": "%path%;e://ana//src//out//debug_x64;E:/github/WebViewExample/out/Debug/x64/WebViewExample/WebViewExample"
            },
             "urlFilter": "*index.html*",
             "useWebView": true
        },
        {
            "name": "Scenario 2: Debug App for Script debugging (targeted) (old adapter)",
            "type": "edge",
            "request": "launch",
            "webRoot": "${workspaceFolder}/webviewexample/src",
            "pathMapping": {
                "E:/github/WebViewExample": "${workspaceFolder}"
            },
            //"runtimeExecutable": "${workspaceFolder}/out/Debug/x64/WebViewExample/WebViewExample.exe",
            "runtimeExecutable": "E:/github/WebViewExample/out/Debug/x64/WebViewExample/WebViewExample.exe",
            "env": {
                    // customize for your build location
                    "Path": "%path%;e://ana//src//out//debug_x64;E:/github/WebViewExample/out/Debug/x64/WebViewExample/WebViewExample"
            },
             "urlFilter": "*index.html?i=2",
            "useWebView": "advanced"
            ,"debugServer": 4712 // only need if your debuging the plugin
        },

        {
            "name": "Scenario 3: Debug App for Script debugging (first) (new adapter)",
            "type": "pwa-msedge",
            "request": "launch",
            "webRoot": "${workspaceFolder}/webviewexample/src",
            "pathMapping": {
                "E:/github/WebViewExample": "${workspaceFolder}"
            },
            //"runtimeExecutable": "${workspaceFolder}/out/Debug/x64/WebViewExample/WebViewExample.exe",
            "runtimeExecutable": "E:/github/WebViewExample/out/Debug/x64/WebViewExample/WebViewExample.exe",
            "env": {
                    // customize for your build location
                    "Path": "%path%;e://ana//src//out//debug_x64;E:/github/WebViewExample/out/Debug/x64/WebViewExample/WebViewExample"
            },
            
                "urlFilter": "about:blank",
                        "useWebView": "advanced"
                        //,"trace": true
             //,"debugServer": 4711 // only need if your debuging the plugin
        }
        {
            "name": "Scenario 4: Debug App for Script debugging (targeted) (new adapter)",
            "type": "pwa-msedge",
            "request": "launch",
            "webRoot": "${workspaceFolder}/webviewexample/src",
            "pathMapping": {
                "E:/github/WebViewExample": "${workspaceFolder}"
            },
            //"runtimeExecutable": "${workspaceFolder}/out/Debug/x64/WebViewExample/WebViewExample.exe",
            "runtimeExecutable": "E:/github/WebViewExample/out/Debug/x64/WebViewExample/WebViewExample.exe",
            "env": {
                    // customize for your build location
                    "Path": "%path%;e://ana//src//out//debug_x64;E:/github/WebViewExample/out/Debug/x64/WebViewExample/WebViewExample"
            },
            
            //"urlFilter": "about:blank",
                         "urlFilter": "*index.html?i=2",
                        "useWebView": "advanced"
                        //,"trace": true
             //,"debugServer": 4711 // only need if your debuging the plugin
        }

    ]
}
