    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug App for C++ debugging",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "out/Debug/x64/WebViewExample/WebViewExample.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "path",
                    "value": "%path%;D:\\edge\\src\\out\\debug_x64"
                }
            ],
            "externalConsole": false
        },
        {
            "type": "edge",
            "request": "launch",
            "name": "Debug App for Script debugging",
            "urlFilter": "*index.html*",
            "webRoot": "${workspaceFolder}",
            "pathMapping": {
                "/git/jalissia/WebViewExample/out/Debug/x64": "${workspaceFolder}"
            },
            "runtimeExecutable": "${workspaceFolder}/out/Debug/x64/WebViewExample/WebViewExample.exe",
            "env": {
                "Path": "%path%;D:\\edge\\src\\out\\debug_x64"
            },
            "useWebView": true
        },
    ]
}
